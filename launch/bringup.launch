<launch>
  <include file="$(find marrtino_r3d)/launch/robot.launch"/> 
 
  <!-- Launching ROSBRidge Server launch   -->
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch"/>
 
  <!-- Launching AprilTAG launch -->
  <include file="$(find marrtino_r3d)/launch/apriltag.launch"/> 
   <!-- Static transforms in the system 
  <node pkg="marrtino_r3d" type="static_broadcaster.py" name="static_broadcaster_node"/>-->
 
   <node name="web_video_server" pkg="web_video_server" type="web_video_server" respawn="false" output="screen">
    <param name="port" value="29090" />
  </node>  
  <!-- Launching teleop launch -->
  <include file="$(find marrtino_r3d)/launch/teleop.launch"/>  
 <!-- Launching RPLIDAR launch -->
  <include file="$(find marrtino_r3d)/launch/rplidar.launch"/>   
 <!-- Launch Speech -->
  <include file="$(find speech)/launch/speech.launch"/>
</launch>
 