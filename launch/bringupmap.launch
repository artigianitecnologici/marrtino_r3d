<!-- Launch file for mapping -->
<launch>
  <!-- robot node -->
  <node pkg="srrg2_orazio_ros" type="orazio_robot_node" name="orazio" output="screen">
      <param name="serial_device" type="string" value="$(optenv ROBOT_DEVICE /dev/orazio)" />
  </node>
  <node pkg="thin_state_publisher" type="thin_state_publisher_node" name="state_pub_robot" args="-odom_topic odom -base_link_frame_id base_frame -odom_frame_id odom $(find marrtino_r3d)/config/$(env ROBOT_NAME)/mapping.txt" >
   </node>
  <!--  da cancellaerw
   <node pkg="thin_state_publisher" type="thin_state_publisher_node" name="state_pub_laser"
	    args="-odom_topic odom -odom_frame_id none $(find marrtino_r3d)/config/$(env ROBOT_NAME)/mapping.txt" output="screen">
   </node> -->
  <!-- Launching teleop launch -->
  <include file="$(find marrtino_r3d)/launch/teleop.launch"/>  
 <!-- Launching RPLIDAR launch -->
  <include file="$(find marrtino_r3d)/launch/rplidar.launch"/>   

</launch>
 
